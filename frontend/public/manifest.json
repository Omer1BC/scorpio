{
  "name": "Scorpio",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["sidePanel", "tabs"],
  "host_permissions": ["http://localhost/*"],
  
  "action": {
      "default_title":"Scorpio"
  },
  "background": {
      "service_worker":"src/background.ts",
      "type": "module"
  },
  "side_panel": {
      "default_path": "index.html"
  }
}
